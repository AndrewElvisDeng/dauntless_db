<div class="container">
  <p id="notice"><%= notice %></p>
</div>

<div class="container">
  <h3> Behemoth name: <%= @behemoth.name %> </h3>
</div>

<div class="container">
  <h5> Elemental Damage: </h5>
  <% @behemoth.elemental_damages.each do |damage| %>
    <p> <%= damage.name %> </p>
  <% end %>
</div>

<div class="container">
  <h5> Elemental Weakness: </h5>
  <% @behemoth.elemental_weaknesses.each do |weakness| %>
    <p> <%= weakness.name %> </p>
  <% end %>
</div>

<div class="container">
  <%= link_to 'Edit', edit_admin_behemoth_path(@behemoth) %> | <%= link_to 'Back', admin_behemoths_path %> 
</div>